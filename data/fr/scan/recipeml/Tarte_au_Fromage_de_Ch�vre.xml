								<unit>servings</unit>
						</yield>
						<preptime type="preparation">
							  <time>
								    <qty>20</qty>
								    <timeunit>minutes</timeunit>
							  </time>
						</preptime>
						<preptime type="cooking">
							  <time>
								    <qty>30</qty>
								    <timeunit>minutes</timeunit>
							  </time>
						</preptime>
        </head>
        <ingredients>
            <ing>
                <amt>
                    <qty>230</qty>
                    <unit>grams</unit>
                </amt>
                <item>flaky pastry </item>
            </ing>
            <ing>
                <amt>
                    <qty>30</qty>
                    <unit>grams</unit>
                </amt>
                <item>butter</item>
            </ing>
            <ing>
                <amt>
                    <qty>30</qty>
                    <unit>grams</unit>
                </amt>
                <item>flour</item>
            </ing>
            <ing>
                <amt>
                    <qty>250</qty>
                    <unit>milliliters</unit>
                </amt>
                <item>milk</item>
            </ing>
            <ing>
                <amt>
                    <qty>3</qty>
                    <unit/>
                </amt>
                <item>chevre cheese</item>
            </ing>
            <ing>
